package com.rmalmeidax.estudos.DTO;

public class AutorListDTO {
	
	private Long id;
	private String nome;
	
	
	public AutorListDTO() {
		super();
	}


	public AutorListDTO(Long id, String nome) {
		this.id = id;
		this.nome = nome;
	}


	public Long getId() {
		return id;
	}


	public String getNome() {
		return nome;
	}
	
	
	
	
	
	

}
